<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="primaryBackColor">#161819</Color>
    <Color x:Key="primaryStrokeColor">#262727</Color>
    <Color x:Key="secondaryBackColor">#242729</Color>
    <Color x:Key="accentColor">#5437DC</Color>
    <Color x:Key="activeAccentColor">#442db3</Color>
    <Color x:Key="primaryFontColor">#FFFFFF</Color>
    <Color x:Key="secondaryFontColor">#7E7E7E</Color>
    
    <SolidColorBrush x:Key="primaryBackColorBrush" Color="{DynamicResource primaryBackColor}"/>
    <SolidColorBrush x:Key="primaryStrokeColorBrush" Color="{DynamicResource primaryStrokeColor}"/>
    <SolidColorBrush x:Key="secondaryBackColorBrush" Color="{DynamicResource secondaryBackColor}"/>
    <SolidColorBrush x:Key="accentColorBrush" Color="{DynamicResource accentColor}"/>
    <SolidColorBrush x:Key="activeAccentColorBrush" Color="{DynamicResource activeAccentColor}"/>
    <SolidColorBrush x:Key="primaryFontColorBrush" Color="{DynamicResource primaryFontColor}"/>
    <SolidColorBrush x:Key="secondaryFontColorBrush" Color="{DynamicResource secondaryFontColor}"/>
    
</ResourceDictionary>